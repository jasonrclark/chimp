use io;
use os;
use assert;
use chimpunit;

main argv {
  chimpunit.test("if positive", fn {
    var executed = false;
    if true {
      executed = true;
    }

    assert.eq(true, executed);
  });

  chimpunit.test("if negative", fn {
    if false {
      assert.fail("Executed false branch!");
    }
  });
}
